<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SurvayArm</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")   
    @Styles.Render("~/Content/themeCss")
</head>
<body class="nav-md">
    <div class="container body">

        <div class="main_container">            
            
                @RenderBody()
            
        </div>

    </div>

   
    
    <script src="~/Scripts/WebTheme/Survay/survay.app.js"></script>
    <script src="~/Scripts/WebTheme/Survay/survay.min.js"></script>
    <script src="~/Scripts/WebTheme/Survay/formBuilder.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqConfirm")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/appCustom")
    @Scripts.Render("~/bundles/pace")  @*bottom up loading bar when reloading the page*@
    @Scripts.Render("~/bundles/notify")
    @RenderSection("scripts", required: false)

    
</body>
</html>
